{
  "name": "simple-email-builder",
  "description": "Simple email builder",
  "private": false,
  "version": "0.0.6",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "typings": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cynsecure/simple-email-builder"
  },
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "v:dev": "vite",
    "v:build": "vite build",
    "v:preview": "vite preview",
    "prepublishOnly": "npm i && npm run compile",
    "postpublish": "npm run clean",
    "compile": "npm run clean && npm run compile:esm && npm run compile:cjs",
    "compile:esm": "tsc",
    "compile:cjs": "tsc --module commonjs --outDir dist/cjs",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@iconify/react": "^6.0.2",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@usewaypoint/block-avatar": "^0.0.3",
    "@usewaypoint/block-button": "^0.0.3",
    "@usewaypoint/block-columns-container": "^0.0.3",
    "@usewaypoint/block-container": "^0.0.2",
    "@usewaypoint/block-divider": "^0.0.4",
    "@usewaypoint/block-heading": "^0.0.3",
    "@usewaypoint/block-html": "^0.0.3",
    "@usewaypoint/block-image": "^0.0.5",
    "@usewaypoint/block-spacer": "^0.0.3",
    "@usewaypoint/block-text": "^0.0.6",
    "@usewaypoint/document-core": "^0.0.6",
    "@usewaypoint/email-builder": "^0.0.8",
    "html2canvas": "^1.4.1",
    "react-colorful": "^5.6.1",
    "swr": "^2.2.5",
    "uuid": "^11.0.5",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
  },
  "peerDependencies": {
    "prettier": ">=2",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
